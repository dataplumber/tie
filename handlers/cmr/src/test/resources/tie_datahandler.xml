<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:aop="http://www.springframework.org/schema/aop"
   xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-2.5.xsd">

   <bean id="dh.fileHandler.factory"
      class="gov.nasa.gibs.tie.handlers.airs.AIRSFileHandlerFactory"/>

   <bean id="dh.metadata.factory"
      class="gov.nasa.gibs.tie.handlers.airs.AIRSMetadataHarvesterFactory"/>

   <bean id="dh.productType.factory"
      class="gov.nasa.gibs.tie.handlers.airs.AIRSProductTypeFactory"/>

   <bean id="dh.workspace.factory" class="gov.nasa.horizon.handlers.framework.LocalWorkspaceFactory"/>

</beans>
